<?php

/**
 * @file
 * Contains update hooks for Thunder Search module.
 */

use Drupal\views\Entity\View;

/**
 * Implements hook_install().
 */
function thunder_search_install() {
  if ($view = View::load('content')) {
    $view->setStatus(FALSE)->save();
  }
  if ($view = View::load('thunder_media')) {
    $view->setStatus(FALSE)->save();
  }
}
