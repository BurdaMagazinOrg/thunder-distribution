{
  "name": "burdamagazinorg/thunder",
  "description": "The thunder distribution",
  "type": "drupal-profile",
  "authors": [
    {
      "name": "Daniel Bosen",
      "email": "daniel.bosen@burda.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0+",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    },
    {
      "type": "vcs",
      "url": "https://git.drupal.org/sandbox/gos77/2677750.git"
    },
    {
      "type": "vcs",
      "url": "https://git.drupal.org/sandbox/gos77/2677752.git"
    }
  ],
  "scripts": {
    "pre-install-cmd": [
      "Thunder\\ScriptHandler::initializeInstaller"
    ],
    "pre-update-cmd": [
      "Thunder\\ScriptHandler::initializeInstaller"
    ]
  },
  "autoload": {
    "classmap": [
      "scripts/ScriptHandler.php"
    ]
  },
  "extra": {
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    }
  },
  "require": {
    "composer/installers": "^1.0",
    "drupal/core": "^8.1.0",
    "drupal/crop": "^8.1.0",
    "drupal/better_normalizers": "dev-8.x-1.x#1cd9b4b50e91f5e595eb86813611dc368227c4b6",
    "drupal/breakpoint_js_settings": "^8.1.0",
    "drupal/admin_toolbar": "^8.1.0",
    "drupal/ctools": "^8.3.0",
    "drupal/default_content": "dev-8.x-1.x#c6a79ee2ffc71b0136d4988871618e05d8be4018",
    "drupal/dropzonejs": "dev-8.x-1.x#23e0d81de407c74c10333e65fb5d4fae09494e09",
    "drupal/entity": "^8.1.0",
    "drupal/entity_browser": "dev-8.x-1.x#77e18608cff42cfc4384bbcdd730a1ffdc37779b",
    "drupal/entity_reference_revisions": "^8.1.0",
    "drupal/fb_instant_articles": "dev-8.x-1.x#07b8afb323332278f5a0c6ba107dee2a81cac4ec",
    "drupal/field_group": "dev-8.x-1.x#77c5dfa28f7a4d7880bedc1cd2accf2d5986697c",
    "drupal/focal_point": "dev-8.x-1.x#e08d5c555f5daf61c35f639762637b119f6a5c8e",
    "drupal/google_analytics": "^8.2.0",
    "drupal/inline_entity_form": "^8.1.0",
    "burdamagazinorg/infinite-module": "dev-8.x-1.x#d19090c8492cfc343b1751da66cd7800f9464948",
    "burdamagazinorg/infinite-theme": "dev-8.x-1.x#e1d6382b7ec73122642cf35d5eb150b7045ed0ed",
    "drupal/ivw_integration": "^8.1.0",
    "drupal/libraries": "dev-8.x-3.x#548f3c2c47596c5242e37b18000e7b2ec930f66a",
    "drupal/linkit": "^8.4.0",
    "drupal/media_entity": "^8.1.0",
    "drupal/media_entity_browser": "dev-8.x-1.x#1f9a47a5b7efacc969367b907e8f033db18bdbae",
    "drupal/media_entity_instagram": "^8.1.0",
    "drupal/media_entity_image": "^8.1.0",
    "drupal/media_entity_slideshow": "^8.1.0",
    "drupal/media_entity_twitter": "^8.1.0",
    "drupal/metatag": "^8.1.0",
    "drupal/paragraphs": "^8.1.0",
    "drupal/pathauto": "^8.1.0",
    "drupal/riddle_marketplace": "^8.1.0",
    "drupal/scheduler": "^8.1.0",
    "drupal/simple_sitemap": "^8.2.0",
    "drupal/token": "^8.1.0",
    "drupal/video_embed_field": "^8.1.0",
    "drupal/views_load_more": "dev-8.x-1.x#178d32195253207cc74097296e25079a23f0b2e5",
    "enyo/dropzone": "^v4.3.0"
  },
  "require-dev": {
    "burdamagazinorg/thunder-dev-tools": "dev-master"
  }
}
